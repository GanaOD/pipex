{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
    			"${workspaceFolder}/inc",
    			"${workspaceFolder}/src",
    			"/usr/local/include",
    			"/Library/Developer/CommandLineTools/usr/include",
    			"${workspaceFolder}/**"
	],
            "defines": [],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX10.15.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-x64",
			"compilerArgs": [
    			"-Wall",
    			"-Wextra",
    			"-Werror",
    			"-g3",
    			"-DDEBUG",
    			"-fno-omit-frame-pointer",
    			"-fsanitize=address"
				]
        }
    ],
    "version": 4
}
